{
  "env": {
    "browser": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "airbnb-base"
  ],
  "rules": {
    "react/sort-prop-types": [
      "error"
    ],
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/jsx-space-before-closing": [
      "error",
      "always"
    ],
    "react/react-in-jsx-scope": [
      "error"
    ]
  }
}
